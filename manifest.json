{
  "name": "AIFiesta Limit Bypass",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Blocks specific requests to AIFiesta API",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": [
    "https://api-v2.aifiesta.ai/*"
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  }
}
